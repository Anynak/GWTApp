<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:m="urn:import:gwt.material.design.client.ui">

    <g:HTMLPanel >
        <m:MaterialCollectionItem type="AVATAR" dismissible="true">
            <m:MaterialLabel ui:field="name" fontSize="2em"/>
            <m:MaterialLabel ui:field="phone"/>
            <m:MaterialLabel ui:field="email"/>
            <m:MaterialLabel ui:field="place"/>
        </m:MaterialCollectionItem>
    </g:HTMLPanel>
</ui:UiBinder>