<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:VerticalPanel ui:field="mainPanel">
        <g:Label text="search settings"/>
        <g:HorizontalPanel>
            <g:Label text="min price"/>
            <g:DoubleBox ui:field='minPriceDoubleBox' title="min price"/>
            <g:Label text="max price"/>
            <g:DoubleBox ui:field='maxPriceDoubleBox' title="max price"/>
        </g:HorizontalPanel>
        <g:Label text="brand"/>
        <g:TextBox ui:field='brandNameTextBox' title="brand"/>
        <g:Label text="model"/>
        <g:TextBox ui:field='modelNameTextBox' title="model"/>
        <g:HorizontalPanel>
            <g:Label text="min release year"/>
            <g:IntegerBox ui:field='releaseYearMinIntBox' title="min release year"/>
            <g:Label text="max release year"/>
            <g:IntegerBox ui:field='releaseYearMaxIntBox' title="max release year"/>
        </g:HorizontalPanel>
        <g:HorizontalPanel>
            <g:Label text="min mileage"/>
            <g:IntegerBox ui:field='mileageMinIntBox' title="min mileage"/>
            <g:Label text="max mileage"/>
            <g:IntegerBox ui:field='mileageMaxIntBox' title="max mileage"/>
        </g:HorizontalPanel>
        <g:Label text="color"/>
        <g:TextBox ui:field='colorTextBox' title="color"/>
    </g:VerticalPanel>
</ui:UiBinder>